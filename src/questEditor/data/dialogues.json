      {
        "id": "boulder_problem",
        "text": "Oh, I see. That's nothing some deep riffs can't handle.",
        "isQuestion": true,
        "isEndOfDialog": false,
        "buttons": [
          {
            "label": "Some riffs?",
            "goToDialog": 3,
            "size": 300
          }
        ]
      },
      {
        "id": "riffs_explanation",
        "text": "Yeah! But my bass needs a new string first. Last gig was too groovy and the old one snapped. Think you can find one?",
        "isQuestion": false,
        "isEndOfDialog": true,
        "onNext": [
          {
            "type": "activateQuest",
            "params": {
              "questId": "deep_tunes"
            }
          }
        ]
      }
    ]
  },
