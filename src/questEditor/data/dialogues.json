  {
    "id": "bassimus_looking_for_my_bass_string",
    "name": "Looking for my Bass String",
    "npcId": "bassimus",
    "questStepId": "talk_to_bassimus",
    "dialogs": [
      {
        "id": "hey_dude",
        "text": "Hey dude, what's up?",
        "isQuestion": true,
        "isEndOfDialog": false,
